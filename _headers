# Cloudflare Pages headers

# Default: don't aggressively cache HTML
/*
  Cache-Control: max-age=0, must-revalidate

# Cache static assets aggressively
/assets/*
  Cache-Control: public, max-age=31536000, immutable

/styles.min.css
  Cache-Control: public, max-age=31536000, immutable

/favicon.png
  Cache-Control: public, max-age=31536000, immutable

/logo.png
  Cache-Control: public, max-age=31536000, immutable
