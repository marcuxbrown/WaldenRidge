{
  "ci": {
    "collect": {
      "staticDistDir": "./",
      "url": [
        "http://localhost/",
        "http://localhost/our-story/",
        "http://localhost/portfolio/",
        "http://localhost/strategy/",
        "http://localhost/leadership/",
        "http://localhost/request/",
        "http://localhost/request/thanks.html",
        "http://localhost/schedule/",
        "http://localhost/ops-intake/"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "budgetsFile": ".github/lighthouse-budget.json"
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    }
  }
}
